<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="foodRatingBar" parent="Widget.AppCompat.RatingBar">


        <item name="android:state_selected">@drawable/star_filled1</item>
        <item name="android:progress">@android:integer/config_longAnimTime</item>
        <item name="android:secondaryProgress">@android:integer/config_shortAnimTime</item>


        <item name="android:numStars">5</item>
        <item name="android:minHeight">30dp</item>
        <item name="android:maxHeight">30dp</item>
        <item name="android:starStyle">@drawable/star_filled1</item>
        <item name="android:itemBackground">@drawable/star_filled1</item>




    </style>



</resources>
